<?xml version="1.0" encoding="UTF-8"?>
<specweb2005Benchmark xmlns:fa="http://faban.sunsource.net/ns/faban"
                      xmlns:fh="http://faban.sunsource.net/ns/fabanharness">
    <!-- jvmConfig is only needed for the cmdAgent since this benchmark is
         not java -->
    <fh:jvmConfig>
        <fh:javaHome>/usr/java</fh:javaHome>
        <fh:jvmOptions> -server -Xmx840m -Xms840m </fh:jvmOptions>
    </fh:jvmConfig>
    <fa:runConfig>
        <fh:description>Sample SPECWEB2005 benchmark run</fh:description>
        <fa:hostConfig>
            <fa:hostPorts>starlost starlost:1200</fa:hostPorts>
            <fh:enabled>true</fh:enabled>
            <fh:cpus>0</fh:cpus>
            <fh:tools>NONE</fh:tools>
            <fh:userCommands/>
            <nodeid>1</nodeid>
            <port>2000</port>
        </fa:hostConfig>
        <clientDir>/export/home/w2005/sw2005-v1.02f.jar</clientDir>
        <fa:scale>50</fa:scale>
        <fa:runControl unit="time">
            <fa:rampUp>60</fa:rampUp>
            <fa:steadyState>120</fa:steadyState>
            <fa:rampDown>60</fa:rampDown>            
        </fa:runControl>
        <testtype>SPECweb_Support</testtype>
        <warmupSeconds>300</warmupSeconds>
        <threadRampupSeconds>300</threadRampupSeconds>
        <!-- threadRampdownSeconds>300</threadRampdownSeconds == rampDown>
        <runSeconds>300</runSeconds == steadyState -->
        <rampupSeconds>300</rampupSeconds>
        <rampdownSeconds>300</rampdownSeconds>
        <iterations>1</iterations>
        <parConns>1</parConns>
        <sleepTestIterations>1</sleepTestIterations>
        <olRun>1</olRun>        
    </fa:runConfig>
    <webserverConfig>
        <fa:hostConfig>
            <fa:host>watergap</fa:host>
            <fh:enabled>true</fh:enabled>
            <fh:cpus>0</fh:cpus>
            <fh:tools>NONE</fh:tools>
            <fh:userCommands/>
        </fa:hostConfig>
        <wsConfig>
            <wsPort>80</wsPort>
            <wsSSLport>443</wsSSLport>
        </wsConfig>
    </webserverConfig>
    <besimserverConfig>
        <fa:hostConfig>
            <fa:host>lindamar</fa:host>
            <fh:enabled>true</fh:enabled>
            <fh:cpus>0</fh:cpus>
            <fh:tools>NONE</fh:tools>
            <fh:userCommands/>
        </fa:hostConfig>
        <besimConfig>
            <port>8000</port>
            <initScript>/BeSim/specweb2005_besim</initScript>
            <persistent>1</persistent>
        </besimConfig>
    </besimserverConfig>
    <supportParameters>
